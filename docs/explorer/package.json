{
  "name": "jabberwocky-explorer",
  "version": "1.0.0",
  "description": "Jabberwocky Bench Explorer",
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "serve": "cd .. && npx http-server . -p 8000 -c-1",
    "dev": "npm run build && npm run serve",
    "build:gh-pages": "webpack --mode production && cp index.html dist/ && cp styles.css dist/",
    "deploy": "npm run build:gh-pages"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^11.0.0"
  }
}